{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  lockFileMaintenance: {
    enabled: true,
  },
  packageRules: [
    {
      matchPackageNames: [
        "diffusers",
        "transformers"
      ],
      enabled: true
    },
    {
      matchPackageNames: [
        "+(*:*|*)"
      ],
      enabled: false
    }
  ]
}